{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "components/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/components/(.*)",
      "dest": "/components/$1"
    },
    {
      "src": "/nav",
      "dest": "/testp.html"
    },
    {
      "src": "/nora",
      "dest": "/test.html"
    },
    {
      "src": "/english",
      "dest": "/english.html"
    },
    {
      "src": "/(.*)",
      "dest": "/openai.html"
    }
  ]
}