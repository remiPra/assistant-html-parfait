{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "components/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/components/(.*)",
      "dest": "/components/$1"
    },
    {
      "src": "/nav",
      "dest": "/testp.html"
    },
    {
      "src": "/testo",
      "dest": "/speechr.html"
    },
    {
      "src": "/gpt4o",
      "dest": "/speechrghpt4.html"
    },
    {
      "src": "/nora",
      "dest": "/test.html"
    },
    {
      "src": "/english",
      "dest": "/english.html"
    },
   
     {
      "src": "/kevin",
      "dest": "/kevin.html"
    },
    {
      "src": "/safari",
      "dest": "/safari.html"
    },
    {
      "src": "/emilie",
      "dest": "/emilie.html"
    },
    {
      "src": "/remi",
      "dest": "/remi.html"
    },
    {
      "src": "/lilian",
      "dest": "/lilianportoflio.html"
    },
    {
      "src": "/sylvaine-site",
      "dest": "/sylvaine.html"
    },
     {
      "src": "/patricerosso",
      "dest": "/patricerosso.html"
    },
     {
      "src": "/prieres",
      "dest": "/prieres.html"
    },
    {
      "src": "/(.*)",
      "dest": "/openai.html"
    }
  ]
}